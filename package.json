{
  "name": "@pencroff/ts-algorithms",
  "version": "0.0.5",
  "description": "Typescript algorithms and Data Structures",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "prepare": "npm run test && npm run build",
    "test": "jest --coverage",
    "docs": "rimraf ./docs && typedoc && npm run copyAssets",
    "copyAssets": "cpx CNAME docs && cpx assets/**/*.{svg,png} docs/assets",
    "pre-commit": "npm run docs",
    "pre-push": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Pencroff/ts-algorithms.git"
  },
  "keywords": [
    "typescript",
    "algorithms"
  ],
  "author": "@Pencroff (Sergey Daniloff)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pencroff/ts-algorithms/issues"
  },
  "homepage": "https://ts-algorithms.pencroff.com",
  "dependencies": {
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "codecov": "^3.6.1",
    "cpx": "^1.5.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "np": "^4.0.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typescript": "^3.7.2"
  }
}
